{
  "rewrites": [
    { "source": "/", "destination": "/frontend/pages/index.html" },
    { "source": "/about", "destination": "/frontend/pages/about.html" },
    { "source": "/services", "destination": "/frontend/pages/services.html" },
    { "source": "/gallery", "destination": "/frontend/pages/gallery.html" },
    { "source": "/projects", "destination": "/frontend/pages/projects.html" },
    { "source": "/faq", "destination": "/frontend/pages/faq.html" },
    { "source": "/contact", "destination": "/frontend/pages/contact.html" }
  ],
  "builds": [
    { "src": "frontend/pages/**/*.html", "use": "@vercel/static" },
    { "src": "frontend/assets/**/*", "use": "@vercel/static" }
  ]
}